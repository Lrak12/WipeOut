{
  "expo": {
    "name": "WipeOut",
    "slug": "wipeout-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logoicon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logosmaller.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yourname.washapp",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAZfI-A_0CamEVloKKjwj4Vq_EzK1Wsj4Y"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logoicon.png",
          "imageWidth": 150,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-build-properties"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "40aad5c3-b871-4c5e-9182-48d60ffa11d5"
      }
    }
  }
}
